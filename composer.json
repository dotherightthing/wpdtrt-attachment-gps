{
    "name": "dotherightthing/wpdtrt-exif",
    "description": "Adds EXIF (time and geotag) fields to the attachment media modal, for use by other plugins.",
    "type": "library",
    "keywords": ["exif", "geotag", "attachment"],
    "license": "ISC",
    "authors": [
        {
            "name": "Dan Smith",
            "email": "dev@dotherightthing.co.nz",
            "homepage": "https://github.com/dotherightthing/wpdtrt-exif",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@dotherightthing.co.nz",
        "issues": "https://github.com/dotherightthing/wpdtrt-exif/issues"
    },
    "require": {
        "dotherightthing/wpdtrt-plugin-boilerplate": "^1.0.0",
        "prairiewest/PHPconvertDMSToDecimal": "dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/dotherightthing/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/prairiewest/PHPconvertDMSToDecimal"
        },
        {
            "packagist.org": false
        }
    ],
    "config": {
        "vendor-dir": "vendor"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "2.9.0",
        "phpunit/phpunit": "^5.7",
        "psy/psysh": "~0.6",
        "wp-cli/wp-cli": "~0.22"
    }
}
